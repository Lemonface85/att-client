{
  "name": "att-client",
  "version": "0.3.1",
  "description": "Node bot library for A Township Tale, a VR game by Alta Reality Pty Ltd.",
  "homepage": "https://github.com/mdingena/att-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdingena/att-client.git"
  },
  "keywords": [
    "township tale",
    "bot library"
  ],
  "author": "Marc Dingena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdingena/att-client/issues"
  },
  "scripts": {
    "test": "ts-node -r dotenv/config test.ts",
    "lint": "eslint \"src/**/*\"",
    "compile": "tsc --noEmit",
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:esm": "tsc -p tsconfig-esm.json",
    "postbuild:cjs": "node ./scripts/cjs-package.js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "jwt-decode": "^3.1.2",
    "tiny-typed-emitter": "^2.1.0",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18.2.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ]
}
